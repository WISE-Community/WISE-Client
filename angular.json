{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wise": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "angular",
              "fabric",
              "dom-autoscroller",
              "@wise-community/drawing-tool",
              "jquery",
              "rxjs/internal/BehaviorSubject",
              "canvg",
              "webfontloader",
              "compute-covariance"
            ],
            "aot": true,
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": [
                  "**/*.scss",
                  "**/*.md"
                ],
                "output": "/assets/"
              },
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/mathjax",
                "output": "/mathjax/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              }
            ],
            "styles": [
              "./node_modules/tabulator-tables/dist/css/tabulator_semanticui.min.css",
              {
                "input": "src/style/styles.scss",
                "bundleName": "siteStyles"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src"
              ]
            },
            "scripts": [],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": false,
              "namedChunks": true,
              "aot": true,
              "localize": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputPath": {
                "base": "dist/"
              },
              "baseHref": "/"
            },
            "development": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": false,
              "budgets": []
            },
            "es": {
              "localize": [
                "es"
              ]
            },
            "ja": {
              "localize": [
                "ja"
              ]
            },
            "pt": {
              "localize": [
                "pt"
              ]
            },
            "tr": {
              "localize": [
                "tr"
              ]
            },
            "zh-Hans": {
              "localize": [
                "zh-Hans"
              ]
            },
            "zh-Hant": {
              "localize": [
                "zh-Hant"
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wise:build",
            "publicHost": "localhost:4200"
          },
          "configurations": {
            "production": {
              "browserTarget": "wise:build:production"
            },
            "development": {
              "browserTarget": "wise:build:development"
            },
            "es": {
              "browserTarget": "wise:build:es"
            },
            "ja": {
              "browserTarget": "wise:build:ja"
            },
            "pt": {
              "browserTarget": "wise:build:pt"
            },
            "tr": {
              "browserTarget": "wise:build:tr"
            },
            "zh-Hans": {
              "browserTarget": "wise:build:zh-Hans"
            },
            "zh-Hant": {
              "browserTarget": "wise:build:zh-Hant"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wise:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "karmaConfig": "karma.conf.js",
            "polyfills": [
              "@angular/localize/init",
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "src/style/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      },
      "i18n": {
        "sourceLocale": {
          "code": "en-US",
          "baseHref": "/"
        },
        "locales": {
          "es": "src/locale/messages.es.xlf",
          "ja": "src/locale/messages.ja.xlf",
          "pt": "src/locale/messages.pt.xlf",
          "tr": "src/locale/messages.tr.xlf",
          "zh-Hans": "src/locale/messages.zh-Hans.xlf",
          "zh-Hant": "src/locale/messages.zh-Hant.xlf"
        }
      }
    },
    "wise-e2e": {
      "root": "ge2e",
      "sourceRoot": "ge2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "wise:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": ""
    }
  },
  "cli": {
    "analytics": false
  }
}